{
  "name": "unnamed-css-preprocessor",
  "version": "1.0.0",
  "main": "lib/Processor.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/unnamed-css-preprocessor.git"
  },
  "author": "4Catalyzer",
  "license": "MIT",
  "scripts": {
    "build": "4c build src",
    "format": "4c format src './*'",
    "lint": "4c lint src './*'",
    "prepublishOnly": "yarn run build",
    "release": "4c release",
    "tdd": "jest --watch",
    "test": "yarn lint && jest && yarn typecheck",
    "testonly": "jest",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "yarn 4c lint --fix",
      "git add"
    ]
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setupAfterEnv.ts"
    ]
  },
  "devDependencies": {
    "@4c/cli": "^2.1.11",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@types/jest": "^26.0.8",
    "@types/node": "14.0.27",
    "@types/resolve": "^1.17.1",
    "dart-sass": "^1.25.0",
    "eslint": "^7.6.0",
    "jest": "^26.2.2",
    "jest-diff": "^26.2.0",
    "memfs": "^3.2.0",
    "pegjs": "^0.10.0",
    "pegjs-backtrace": "^0.2.0",
    "prettier": "^2.0.1",
    "ts-pegjs": "^0.2.7",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@csstools/convert-colors": "^2.0.0",
    "@modular-css/processor": "^25.8.0",
    "@modular-css/shortnames": "^25.0.0",
    "@types/color-convert": "^1.9.0",
    "@types/d3-color": "^1.2.2",
    "@types/lodash": "^4.14.158",
    "acorn": "^7.4.0",
    "browserslist": "^4.13.0",
    "caniuse-lite": "^1.0.30001109",
    "color-convert": "^2.0.1",
    "d3-color": "^1.4.1",
    "dependency-graph": "^0.9.0",
    "enhanced-resolve": "^5.0.0-beta.10",
    "escape-string-regexp": "^4.0.0",
    "fastest-levenshtein": "^1.0.10",
    "khroma": "^1.1.0",
    "lodash": "^4.17.19",
    "postcss": "^7.0.32",
    "postcss-scss": "^2.1.1",
    "postcss-selector-parser": "^6.0.2",
    "postcss-value-parser": "^4.1.0",
    "postcss-values-parser": "^3.2.1",
    "resolve": "^1.17.0",
    "resolve-from": "^5.0.0",
    "synchronous-promise": "^2.0.13",
    "unique-slug": "^2.0.2",
    "yargs": "^15.4.1"
  }
}
